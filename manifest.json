{
  "name": "CollegeConfetti",
  "description": "Admission Letter Stress Be Gone!",
  "version": "1.0.1",
  "icons": {
    "128": "128.png",
    "48": "48.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*.edu/*", "https://*.edu/*"],
      "js": ["jquery.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://*.edu/*",
    "https://*.edu/*"
  ],
  "author": "Eric Qian",
  "homepage_url": "https://bananiumlabs.com",
  "manifest_version": 2
}
